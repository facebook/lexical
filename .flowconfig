[ignore]
.*/scripts/.*
.*/build/.*
.*/__tests__/.*
.*/dist/.*
.*/.tempUserDataDir/.*
.*/node_modules/.*

[include]
./packages/

[lints]
untyped-type-import=error

[options]
server.max_workers=4

module.name_mapper='^lexical$' -> '<PROJECT_ROOT>/packages/lexical/src/core/index.js'

module.name_mapper='^lexical/HeadingNode' -> '<PROJECT_ROOT>/packages/lexical/src/extensions/LexicalHeadingNode.js'
module.name_mapper='^lexical/ListNode' -> '<PROJECT_ROOT>/packages/lexical/src/extensions/LexicalListNode.js'
module.name_mapper='^lexical/ListItemNode' -> '<PROJECT_ROOT>/packages/lexical/src/extensions/LexicalListItemNode.js'
