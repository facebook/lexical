[ignore]
.*/scripts/.*
.*/build/.*
.*/__tests__/.*
.*/dist/.*
.*/.tempUserDataDir/.*
.*/node_modules/.*

[include]
./packages/

[lints]
untyped-type-import=error

[options]
server.max_workers=4
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable

module.name_mapper='^outline$' -> '<PROJECT_ROOT>/packages/outline/src/index.js'

module.name_mapper='^outline-react/useOutlineRichText' -> '<PROJECT_ROOT>/packages/outline-react/src/useOutlineRichText.js'
module.name_mapper='^outline-react/useOutlinePlainText' -> '<PROJECT_ROOT>/packages/outline-react/src/useOutlinePlainText.js'
module.name_mapper='^outline-react/useOutlineFocusIn' -> '<PROJECT_ROOT>/packages/outline-react/src/useOutlineFocusIn.js'
module.name_mapper='^outline-react/useOutlineEditorEvents' -> '<PROJECT_ROOT>/packages/outline-react/src/useOutlineEditorEvents.js'
module.name_mapper='^outline-react/OutlineHotKeys' -> '<PROJECT_ROOT>/packages/outline-react/src/OutlineHotKeys.js'
module.name_mapper='^outline-react/OutlineEnv' -> '<PROJECT_ROOT>/packages/outline-react/src/OutlineEnv.js'
module.name_mapper='^outline-react/OutlineSelectionHelpers' -> '<PROJECT_ROOT>/packages/outline-react/src/OutlineSelectionHelpers.js'
module.name_mapper='^outline-react/OutlineEventHandlers' -> '<PROJECT_ROOT>/packages/outline-react/src/OutlineEventHandlers.js'

module.name_mapper='^outline-extensions/HeadingNode' -> '<PROJECT_ROOT>/packages/outline-extensions/src/OutlineHeadingNode.js'
module.name_mapper='^outline-extensions/ListNode' -> '<PROJECT_ROOT>/packages/outline-extensions/src/OutlineListNode.js'
module.name_mapper='^outline-extensions/ImageNode' -> '<PROJECT_ROOT>/packages/outline-extensions/src/OutlineImageNode.js'
module.name_mapper='^outline-extensions/QuoteNode' -> '<PROJECT_ROOT>/packages/outline-extensions/src/OutlineQuoteNode.js'
module.name_mapper='^outline-extensions/ParagraphNode' -> '<PROJECT_ROOT>/packages/outline-extensions/src/OutlineParagraphNode.js'

suppress_type=$FlowIssue
suppress_type=$FlowFixMe
suppress_type=$FixMe
suppress_type=$FlowExpectedError


[libs]
./libdefs/

[strict]
nonstrict-import
unclear-type
