module.name_mapper='^lexical/TableRowNode' -> '<PROJECT_ROOT>/packages/lexical/src/extensions/LexicalTableRowNode.js'
module.name_mapper='^lexical/TableCellNode' -> '<PROJECT_ROOT>/packages/lexical/src/extensions/LexicalTableCellNode.js'
module.name_mapper='^lexical/QuoteNode' -> '<PROJECT_ROOT>/packages/lexical/src/extensions/LexicalQuoteNode.js'
module.name_mapper='^lexical/ParagraphNode' -> '<PROJECT_ROOT>/packages/lexical/src/extensions/LexicalParagraphNode.js'
module.name_mapper='^lexical/CodeNode' -> '<PROJECT_ROOT>/packages/lexical/src/extensions/LexicalCodeNode.js'
module.name_mapper='^lexical/LinkNode' -> '<PROJECT_ROOT>/packages/lexical/src/extensions/LexicalLinkNode.js'
module.name_mapper='^lexical/HashtagNode' -> '<PROJECT_ROOT>/packages/lexical/src/extensions/LexicalHashtagNode.js'

