import type {
  EditorConfig,
  LexicalNode,
  NodeKey,
  SerializedTextNode,
} from 'lexical';

import {TextNode} from 'lexical';

declare export class SpinNode extends TextNode {
  static getType(): string;
  // $FlowFixMe
  static clone(node: SpinNode): SpinNode;
  static importJSON(serializedNode: SerializedTextNode): SpinNode;
  constructor(text: string, key?: NodeKey): void;
