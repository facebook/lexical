[ignore]
.*/scripts/.*
.*/build/.*
.*/__tests__/.*
.*/dist/.*
.*/.tempUserDataDir/.*
.*/node_modules/.*

[include]
./packages/

[lints]
untyped-type-import=error

[options]
server.max_workers=4

module.name_mapper='^lexical$' -> '<PROJECT_ROOT>/packages/lexical-core/src/index.js'

module.name_mapper='^lexical/HeadingNode' -> '<PROJECT_ROOT>/packages/lexical-core/src/nodes/extended/LexicalHeadingNode.js'
