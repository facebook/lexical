[ignore]
.*/scripts/.*
.*/build/.*
.*/__tests__/.*
.*/dist/.*
.*/.tempUserDataDir/.*
.*/node_modules/.*

[include]
./packages/

[lints]
untyped-type-import=error

[options]
server.max_workers=4

module.name_mapper='^outline$' -> '<PROJECT_ROOT>/packages/outline/src/core/index.js'

module.name_mapper='^outline/HeadingNode' -> '<PROJECT_ROOT>/packages/outline/src/extensions/OutlineHeadingNode.js'
module.name_mapper='^outline/ListNode' -> '<PROJECT_ROOT>/packages/outline/src/extensions/OutlineListNode.js'
module.name_mapper='^outline/ListItemNode' -> '<PROJECT_ROOT>/packages/outline/src/extensions/OutlineListItemNode.js'
module.name_mapper='^outline/QuoteNode' -> '<PROJECT_ROOT>/packages/outline/src/extensions/OutlineQuoteNode.js'
module.name_mapper='^outline/ParagraphNode' -> '<PROJECT_ROOT>/packages/outline/src/extensions/OutlineParagraphNode.js'
module.name_mapper='^outline/CodeNode' -> '<PROJECT_ROOT>/packages/outline/src/extensions/OutlineCodeNode.js'
module.name_mapper='^outline/LinkNode' -> '<PROJECT_ROOT>/packages/outline/src/extensions/OutlineLinkNode.js'
module.name_mapper='^outline/HashtagNode' -> '<PROJECT_ROOT>/packages/outline/src/extensions/OutlineHashtagNode.js'

module.name_mapper='^outline/history' -> '<PROJECT_ROOT>/packages/outline/src/helpers/OutlineHistoryHelpers.js'
module.name_mapper='^outline/selection' -> '<PROJECT_ROOT>/packages/outline/src/helpers/OutlineSelectionHelpers.js'
module.name_mapper='^outline/text' -> '<PROJECT_ROOT>/packages/outline/src/helpers/OutlineTextHelpers.js'
module.name_mapper='^outline/keys' -> '<PROJECT_ROOT>/packages/outline/src/helpers/OutlineKeyHelpers.js'
module.name_mapper='^outline/nodes' -> '<PROJECT_ROOT>/packages/outline/src/helpers/OutlineNodeHelpers.js'
module.name_mapper='^outline/events' -> '<PROJECT_ROOT>/packages/outline/src/helpers/OutlineEventHelpers.js'
module.name_mapper='^outline/offsets' -> '<PROJECT_ROOT>/packages/outline/src/helpers/OutlineOffsetHelpers.js'
module.name_mapper='^outline/root' -> '<PROJECT_ROOT>/packages/outline/src/helpers/OutlineRootHelpers.js'

module.name_mapper='^outline-react/OutlineTreeView' -> '<PROJECT_ROOT>/packages/outline-react/src/OutlineTreeView.js'
module.name_mapper='^outline-react/useOutlineEditor' -> '<PROJECT_ROOT>/packages/outline-react/src/useOutlineEditor.js'
module.name_mapper='^outline-react/useOutlineRichText' -> '<PROJECT_ROOT>/packages/outline-react/src/useOutlineRichText.js'
module.name_mapper='^outline-react/useOutlinePlainText' -> '<PROJECT_ROOT>/packages/outline-react/src/useOutlinePlainText.js'
module.name_mapper='^outline-react/useOutlineRichTextWithCollab' -> '<PROJECT_ROOT>/packages/outline-react/src/useOutlineRichTextWithCollab.js'
module.name_mapper='^outline-react/useOutlineEditorEvents' -> '<PROJECT_ROOT>/packages/outline-react/src/useOutlineEditorEvents.js'
module.name_mapper='^outline-react/useOutlineAutoFormatter' -> '<PROJECT_ROOT>/packages/outline-react/src/useOutlineAutoFormatter.js'
module.name_mapper='^outline-react/useOutlineDecorators' -> '<PROJECT_ROOT>/packages/outline-react/src/useOutlineDecorators.js'
module.name_mapper='^outline-react/useOutlineNestedList' -> '<PROJECT_ROOT>/packages/outline-react/src/useOutlineNestedList.js'
module.name_mapper='^outline-react/useOutlineIsBlank' -> '<PROJECT_ROOT>/packages/outline-react/src/useOutlineIsBlank.js'
module.name_mapper='^outline-react/useOutlineIsTextContentEmpty' -> '<PROJECT_ROOT>/packages/outline-react/src/useOutlineIsTextContentEmpty.js'
module.name_mapper='^outline-react/useOutlineCanShowPlaceholder' -> '<PROJECT_ROOT>/packages/outline-react/src/useOutlineCanShowPlaceholder.js'
module.name_mapper='^outline-react/useOutlineCharacterLimit' -> '<PROJECT_ROOT>/packages/outline-react/src/useOutlineCharacterLimit.js'

module.name_mapper='^outline-yjs$' -> '<PROJECT_ROOT>/packages/outline-yjs/src/index.js'

module.name_mapper='^shared/isImmutableOrInert' -> '<PROJECT_ROOT>/packages/shared/src/isImmutableOrInert.js'
module.name_mapper='^shared/invariant' -> '<PROJECT_ROOT>/packages/shared/src/invariant.js'
module.name_mapper='^shared/getDOMTextNodeFromElement' -> '<PROJECT_ROOT>/packages/shared/src/getDOMTextNodeFromElement.js'
module.name_mapper='^shared/environment' -> '<PROJECT_ROOT>/packages/shared/src/environment.js'
module.name_mapper='^shared/getPossibleDecoratorNode' -> '<PROJECT_ROOT>/packages/shared/src/getPossibleDecoratorNode.js'

munge_underscores=false

suppress_type=$FlowIssue
suppress_type=$FlowFixMe
suppress_type=$FixMe
suppress_type=$FlowExpectedError

[libs]
./libdefs/
./scripts/flow/environment.js

[strict]
nonstrict-import
unclear-type
